<generatorConfiguration name ='world-root'>
    <storeFile>/src/main/resources/world.xml</storeFile>
    <generators>
        <generator>
            <class>com.shpandrak.codegen.schema.SchemaCodeGenerator</class>
        </generator>
        <generator>
            <class>com.shpandrak.codegen.PojoGenerator</class>
        </generator>
        <generator>
            <class>com.shpandrak.codegen.DatabaseLayerGenerator</class>
            <properties>
                <property>
                    <name>connection-provider</name>
                    <value>com.shpandrak.integrationtests.TestJDBCConnectionProvider</value>
                </property>
            </properties>
        </generator>
        <generator>
            <class>com.shpandrak.codegen.GoogleDatastoreLayerGenerator</class>
        </generator>
        <generator>
            <class>com.shpandrak.codegen.xml.XMLConvertersGenerator</class>
        </generator>
        <generator>
            <class>com.shpandrak.codegen.rest.RestLayerGenerator</class>
            <properties>
                <property>
                    <name>persistence-layer-generator</name>
                    <value>com.shpandrak.codegen.DatabaseLayerGenerator</value>
                </property>
            </properties>
        </generator>
        <generator>
            <class>com.shpandrak.codegen.webmanage.WebManageGenerator</class>
        </generator>
    </generators>
</generatorConfiguration>